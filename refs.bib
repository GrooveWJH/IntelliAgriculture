@article{DuanKeJun_JiYubpShenJingWangLuopidKongZhiQiDeNongYeWenShiXiTongFangZhenYanJiu_2025,
  title = {基于BP神经网络PID控制器的农业温室系统仿真研究},
  author = {{段科俊} and {李小丽} and {金光哲} and {余侠} and {张扬}},
  date = {2025},
  journaltitle = {南方农机},
  volume = {56},
  number = {6},
  pages = {18--22},
  url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CJFQ&dbname=CJFDLAST2025&filename=NFLJ202506005},
  urldate = {2025-05-15},
  abstract = {【目的】传统控制方法存在控制精度低、系统输出不稳定、明显大滞后等问题，亟需探索一种适用于现代农业温室生产系统的合理有效的控制策略。【方法】首先，根据BP神经网络原理确定BP-PID控制器的模型；其次，结合不同的隐含层神经元计算公式，确定不同数量的隐含层神经元节点，并通过MATLAB软件对不同隐含层神经元节点数量的BP-PID算法进行仿真；最后，分析BP神经网络中隐含层神经元节点数量对控制结果的影响，并确定仿真模型的隐含层神经元节点阈值区间。【结果】在3输入、3输出的BP-PID控制中，隐含层神经元节点较少时(q≤3)，系统的响应曲线不够平滑，控制精度较低；隐含层神经元节点过多时(q≥8)，系统响应曲线的平滑度较好，但控制精度难以进一步提升，超调现象也难以进一步改善。【结论】隐含层神经元节点q=6时具有较好的控制效果，虽然不同数量的隐含层神经元节点在控制系统中的效果不尽相同，但整体效果均优于常规PID控制，BP-PID控制适用于具有延时环节的一阶惯性温室系统。},
  langid = {chinese},
  keywords = {BP-PID,MATLAB,温室,神经网络,隐含层},
  annotation = {foundation: 云南省教育厅科研基金项目资助“基于人工神经网络的农业温室控制系统研究”(2023J1845)；\\
download: 590\\
CLC: S625;TP273;TP183\\
dbcode: CJFQ\\
dbname: CJFDLAST2025\\
filename: NFLJ202506005},
  file = {/Users/groove/Library/CloudStorage/GoogleDrive-groovewjh@gmail.com/My Drive/ZoteroBase/段科俊 et al. - 2025 - 基于BP神经网络PID控制器的农业温室系统仿真研究.pdf}
}

